//computing fubonacci sequence
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a=0,b=1,c;
    int n;
    cin>>n;
    if(n==1||n==2)
    {
        cout<<1;
    }
    else
    {
        for(int i=2;i<=n;i++)
        {
            c=a+b;
            a=b;
            b=c;
        }
        cout<<c;
    }
    
    return 0;
}
