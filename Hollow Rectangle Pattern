// Sample Input 0

// 5 4
// Sample Output 0

// *****
// *   *
// *   *
// *****




#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int w,l;
    cin>>w>>l;
    for(int i=0;i<l;i++){
        for(int j=0;j<w;j++){
            if(i==0 || j==0 || i==l-1 || j==w-1){
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}
