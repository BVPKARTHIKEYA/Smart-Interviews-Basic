// we will read the array and if index is divisible by 2 we will add 0 to sum else we will add the sum to that array
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    int a[n],i,sum=0;
    for(i=0;i<n;i++)
    { 
        cin>>a[i];
        if(a[i]%2!=0)
        {
            sum+=a[i];
         }
        else{
            sum+=0;
          }
     }
    cout<<sum;
    
    
    return 0;
}
