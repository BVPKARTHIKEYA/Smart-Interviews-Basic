
//Rectangle Pattern

// Sample Input 0

// 5
// Sample Output 0

// 5432*
// 543*1
// 54*21
// 5*321
// *4321
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    int x=1;
   for(int i=0;i<n;i++)
   {
       
       for(int j=n;j>=1;j--)
       {
           
           if(j==x)
           {
               cout<<"*";
               
           }
           else
           {
            cout<<j;
           }
          
       }
        x++;
       cout<<endl;
   }
    return 0;
}
