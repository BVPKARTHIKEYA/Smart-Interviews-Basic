//checeking whether the given triangle is valid or not
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    unsigned long long a,b,c;
    cin>>a>>b>>c;
    if(a<1||b<1||c<1)
    {
        cout<<"No";
    }
    if(a+b>c && c+a>b && b+c>a)
    {
        cout<<"Yes";
        
    }
    else
    {
        cout<<"No";
    }
    return 0;
}
