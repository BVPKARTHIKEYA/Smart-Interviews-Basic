//computing area of rectangle
// area of a rectangle = l*b
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    long int l=0,b=0;
    cin>>l>>b;
    if(l>=1&&b<=pow(10,9))
    {
        cout<<l*b;
    }   
    
    return 0;
}
